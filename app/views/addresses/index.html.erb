<p id="notice"><%= notice %></p>

<h1>Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Building</th>
      <th>Cadasref</th>
      <th>Cienum</th>
      <th>Sodecinum</th>
      <th>Ilot</th>
      <th>Lot</th>
      <th>Street</th>
      <th>Postcode</th>
      <th>Quarter</th>
      <th>Postagencenum</th>
      <th>Streetnum</th>
      <th>Stagenum</th>
      <th>Doornum</th>
      <th>Country</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.building %></td>
        <td><%= address.cadasRef %></td>
        <td><%= address.cieNum %></td>
        <td><%= address.sodeciNum %></td>
        <td><%= address.ilot %></td>
        <td><%= address.lot %></td>
        <td><%= address.street %></td>
        <td><%= address.postCode %></td>
        <td><%= address.quarter %></td>
        <td><%= address.postAgenceNum %></td>
        <td><%= address.streetNum %></td>
        <td><%= address.stageNum %></td>
        <td><%= address.doorNum %></td>
        <td><%= address.country %></td>
        <td><%= address.city %></td>
        <td><%= link_to 'Show', address %></td>
        <td><%= link_to 'Edit', edit_address_path(address) %></td>
        <td><%= link_to 'Destroy', address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address', new_address_path %>
